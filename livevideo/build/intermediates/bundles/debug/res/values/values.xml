<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="app_name">LiveVideo</string>
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light">
	    <!-- 
	    	Theme customizations available in newer API levels can go in
	    	res/values-vXX/styles.xml, while customization related to
	    	backward-compatibility can go here.
	     -->
	</style>
    <style name="AppTheme" parent="AppBaseTheme">
	    <!-- All customizations that are NOT specific to a particular API-level can go here. -->
	</style>
    <style name="MediaController_SeekBar" parent="android:Widget.SeekBar">
        <item name="android:progressDrawable">@drawable/media_controller_progressbar</item>
        <item name="android:indeterminateDrawable">@drawable/media_controller_progressbar</item>
        <item name="android:minHeight">5dp</item>
        <item name="android:maxHeight">5dp</item>
        <item name="android:thumb">@drawable/media_controller_progressbar_thumb</item>
        <item name="android:thumbOffset">8dp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
    </style>
</resources>